# Figure params
figure.constrained_layout.use: True
figure.facecolor: white
figure.figsize: 5.69, 3.52
figure.dpi: 200

# Font
font.family: serif

# Fontsize
font.size: 8
axes.labelsize: 7
axes.titlesize: 8
legend.fontsize: 6
legend.title_fontsize: 7
xtick.labelsize: 6
ytick.labelsize: 6

# Latex
text.usetex: True
text.latex.preamble: \usepackage{amsmath} \usepackage{amssymb}

# Savefig params
savefig.bbox: standard
savefig.dpi: 300
savefig.format: pdf
savefig.pad_inches: 0.05

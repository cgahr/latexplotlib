# Figure params
figure.constrained_layout.use: True
figure.facecolor: white
figure.figsize: 5.69, 3.52
figure.dpi: 200

# Font
font.family: serif

# Fontsize
font.size: 10
axes.labelsize: 9
axes.titlesize: 10
legend.fontsize: 8
legend.title_fontsize: 9
xtick.labelsize: 8
ytick.labelsize: 8

# Latex
text.usetex: True
text.latex.preamble: \usepackage{amsmath} \usepackage{amssymb}

# Savefig params
savefig.bbox: standard
savefig.dpi: 300
savefig.format: pdf
savefig.pad_inches: 0.05

# Figure params
figure.constrained_layout.use: True
figure.facecolor: white
figure.figsize: 5.69, 3.52
figure.dpi: 200

# Font
font.family: serif

# Fontsize
font.size: 11
axes.labelsize: 10
axes.titlesize: 11
legend.fontsize: 9
legend.title_fontsize: 10
xtick.labelsize: 9
ytick.labelsize: 9

# Latex
text.usetex: True
text.latex.preamble: \usepackage{amsmath} \usepackage{amssymb}

# Savefig params
savefig.bbox: standard
savefig.dpi: 300
savefig.format: pdf
savefig.pad_inches: 0.05

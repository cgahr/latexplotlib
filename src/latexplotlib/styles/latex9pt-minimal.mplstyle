# Figure params
figure.constrained_layout.use: True
figure.facecolor: white
figure.figsize: 5.69, 3.52
figure.dpi: 200

# Font
font.family: serif

# Fontsize
font.size: 7
axes.labelsize: 6
axes.titlesize: 7
legend.fontsize: 5
legend.title_fontsize: 6
xtick.labelsize: 5
ytick.labelsize: 5

# Latex
text.usetex: True
text.latex.preamble: \usepackage{amsmath} \usepackage{amssymb}

# Savefig params
savefig.bbox: standard
savefig.dpi: 300
savefig.format: pdf
savefig.pad_inches: 0.05

# Figure params
figure.constrained_layout.use: True
figure.facecolor: white
figure.figsize: 5.69, 3.52
figure.dpi: 200

# Font
font.family: serif

# Fontsize
font.size: 9
axes.labelsize: 8
axes.titlesize: 9
legend.fontsize: 7
legend.title_fontsize: 8
xtick.labelsize: 7
ytick.labelsize: 7

# Latex
text.usetex: True
text.latex.preamble: \usepackage{amsmath} \usepackage{amssymb}

# Savefig params
savefig.bbox: standard
savefig.dpi: 300
savefig.format: pdf
savefig.pad_inches: 0.05
